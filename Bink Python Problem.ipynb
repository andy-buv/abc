    "s = 'abcccabcdezbc'\n",
    "l = []\n",
    "\n",
    "def alpha(s,l):\n",
    "    for i in range(len(s)-1):\n",
    "        if s[i] <= s[i + 1]:\n",
    "            l.append(s[i])\n",
    "        else: \n",
    "            pass\n",
    "    return(l)\n",
    "\n",
    "print(alpha(s,l))"
